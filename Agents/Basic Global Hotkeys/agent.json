{
    "$schema": "http://www.lavishsoft.com/schema/agent.json",
    "name": "Basic Global Hotkeys",
    "platforms": {
        "joe multiboxer uplink": {
            "eventHandlers": {
                "onAgentStartup": {
                    "code": [
                        "run BGH.Uplink.iss"
                    ]
                },
                "onAgentShutdown": {
                    "code": [
                        "endscript BGH.Uplink"
                    ]
                }
            }
        },
        "joe multiboxer session": {
            "eventHandlers": {
                "onAgentStartup": {
                    "code": [
                        "run BGH.Session.iss"
                    ]
                },
                "onAgentShutdown": {
                    "code": [
                        "endscript BGH.Session"
                    ]
                }
            }
        }
    }
}